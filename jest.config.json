{
  "transform": {
    "^.+\\.tsx?$": "ts-jest",
    ".*\\.(ts)$":".*\\.(ts)$': '<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "collectCoverageFrom": [
    "src/*.{ts,tsx}",
    "!**/node_modules/**",
    "!**/vendor/**"
  ],
  "coverageDirectory": "test/"
}